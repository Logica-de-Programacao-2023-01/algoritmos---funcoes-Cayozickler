import "errors"

func aplicarFuncao(slice []int, funcao func(int) int) ([]int, error) {
    if len(slice) == 0 {
        return nil, errors.New("O slice est√° vazio")
    }

    resultado := make([]int, len(slice))
    for i, valor := range slice {
        resultado[i] = funcao(valor)
    }

    return resultado, nil
}
